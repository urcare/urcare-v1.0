
{
  "functions": {
    "api/phonepe-pay.js": {
      "maxDuration": 30
    },
    "api/phonepe-status.js": {
      "maxDuration": 30
    },
    "api/phonepe-callback.js": {
      "maxDuration": 30
    },
    "api/generate-plan.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/phonepe/pay", "destination": "/api/phonepe-pay.js" },
    { "source": "/api/phonepe/status", "destination": "/api/phonepe-status.js" },
    { "source": "/api/phonepe/callback", "destination": "/api/phonepe-callback.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
