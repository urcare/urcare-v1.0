
{
  "functions": {
    "api/generate-plan.js": {
      "maxDuration": 30
    },
    "api/phonepe/pay.js": {
      "maxDuration": 30
    },
    "api/phonepe/status.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
