
{
  "functions": {
    "api/razorpay-create-order.js": {
      "maxDuration": 30
    },
    "api/razorpay-verify-payment.js": {
      "maxDuration": 30
    },
    "api/generate-plan.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/razorpay/create-order", "destination": "/api/razorpay-create-order.js" },
    { "source": "/api/razorpay/verify-payment", "destination": "/api/razorpay-verify-payment.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
