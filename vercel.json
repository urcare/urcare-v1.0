{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "src/pages/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!robots\\.txt|favicon\\.ico|sw\\.js|images/|icons/|brand\\.png|brand\\.jpg|logoAnimation\\.json|placeholder\\.svg|qr-code-.*|urcare-logo\\.svg|api/).*)",
      "destination": "/index.html"
    }
  ]
}